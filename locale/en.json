	"invalidConstructorAction@proto": "constructorAction option must be 'error', 'ignore', 'preserve' or undefined, value: {{-value,valueType}}",
	"invalidConstructorAction@hades": "~[constructorAction] option must be ~{'error'}, ~{'ignore'}, ~{'preserve'} or ~{undefined}, ~[value] {{-value,valueType@hades}}",
	"errorSyntax": {
		"unexpectedChar@proto": "syntax error, expected char '{{charExpected}}' instead of '{{char}}', index: {{index}}",
